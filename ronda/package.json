{
  "name": "@forrest-gump/ronda",
  "displayName": "Ronda",
  "description": "Tooling for forrest-gump",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ronda.newFeature"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ronda.newFeature",
        "title": "Create a new feature"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  }
}
